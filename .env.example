# Configurações de acesso ao Steam Market

# Código da moeda para exibição de preços
STEAM_MARKET_CURRENCY=7  # 7 = BRL (Real Brasileiro)

# ID da aplicação CS2
STEAM_APPID=730

# Tempo mínimo (em segundos) entre requisições ao endpoint do mercado
# para evitar rate limiting (bloqueio por excesso de requisições)
STEAM_REQUEST_DELAY=1.0
